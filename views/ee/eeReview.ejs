			<% } else { %>
				<i></i><span class="label featured__label label-info">Free</span>
			<% } %>
	        </div>
	      </div>
	    </section>
	    <section>
	      <div class="container">
	        <div class="row">
	          <div class="col-lg-8">
	            <h3>Description</h3>
	            <p><%= listing.description %></p>
	            
	            <div class="job-detail__apply-bottom"><a href="#" class="btn btn-primary">Apply for this job</a></div>
	          </div>
	        </div>
	      </div>
	    </section>
	   
      </div>
    </div> 
  <% include ../partials/footer %>
